# Frontend environment variables (example - do not commit secrets)

# PUBLIC_INTERFACE
# Base URL for backend API. Leave blank to use same-origin (dev proxy or deployed same domain).
REACT_APP_BACKEND_URL=https://your-backend.example.com

# Optional variables referenced by the platform or other tooling (not strictly required for this step)
REACT_APP_API_BASE=
REACT_APP_FRONTEND_URL=
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=true
